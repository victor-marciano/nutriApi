{
    "env": {
        "es2020": true,
        "node": true
    },
    "plugins": ["prettier"],
    "extends": ["standard", "prettier/prettier"],
    "parser": "babel-eslint",
    "parserOptions": {
        "ecmaVersion": 11,
        "sourceType": "module"
    },
    "rules": {
        "prettier/prettier": "error"
    }
}
